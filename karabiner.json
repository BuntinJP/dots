{
  "title": "wasdで矢印操作",
  "rules": [
    {
      "description": "descriptionだけ記述",
      "manipulators": [{}]
    },
    {
      "description": "right_shiftとaで左矢印",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "a",
            "modifiers": { "mandatory": ["right_shift"] }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "right_shiftとwで上矢印",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": { "mandatory": ["right_shift"] }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "right_shiftとdで右矢印",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "d",
            "modifiers": { "mandatory": ["right_shift"] }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ]
        }
      ]
    },
    {
      "description": "right_shiftとsで下矢印",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "s",
            "modifiers": { "mandatory": ["right_shift"] }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ]
        }
      ]
    }
  ]
}
